{
  "$schema": "../schemas/config.schema.json",
  "baseUrl": "https://meet.jit.si",
  "locationString": {"default": "string"},
  "meetings": [
    {
      "type": "StandardMeeting",
      "additionalConfig": {
        "config.startWithAudioMuted": true,
        "config.startWithVideoMuted": true
      },
      "meetingHeader": {"default": "Test Meeting"},
      "additionalTexts": [
        {
          "fontSize": "12px",
          "fontFamily": "Segoe UI",
          "fontColor": "#ffffff",
          "texts": [
            {
              "addNewLine": true,
              "text": {"default": "Test this meeting"}
            },
            {
              "addNewLine": false,
              "text": {"default": "Url to wikipedia"},
              "url": {"default": "https://wikipedia.com"}
            }
          ]
        }
      ]
    }
  ],
  "fontFamily": "Arial",
  "fontSize": "20px",
  "fontColor": "#0d85c7",
  "divColor": "#0d85c7"
}
