{
  "name": "jitsi-outlook-addin",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-TaskPane.git"
  },
  "license": "MIT",
  "config": {
    "app_to_debug": "outlook",
    "app_type_to_debug": "desktop",
    "dev_server_port": 3000
  },
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "dev-server": "webpack serve --mode development",
    "lint": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "prettier": "office-addin-lint prettier",
    "start": "office-addin-debugging start manifest.xml",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "start:web": "office-addin-debugging start manifest.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "validate": "office-addin-manifest validate",
    "watch": "webpack --mode development --watch",
    "test": "jest",
    "test-config": "ts-node build-utils/test-footer-config.ts"
  },
  "dependencies": {
    "core-js": "^3.46.0",
    "regenerator-runtime": "^0.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@jridgewell/source-map": "^0.3.11",
    "@types/jest": "^30.0.0",
    "@types/office-js": "^1.0.557",
    "@types/office-runtime": "^1.0.23",
    "acorn": "^8.15.0",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "eslint-plugin-office-addins": "^4.0.3",
    "file-loader": "^6.2.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.4",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "office-addin-cli": "^2.0.3",
    "office-addin-debugging": "^6.0.6",
    "office-addin-dev-certs": "^2.0.3",
    "office-addin-lint": "^3.0.3",
    "office-addin-manifest": "^2.0.3",
    "office-addin-mock": "^3.0.6",
    "office-addin-prettier-config": "^2.0.1",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "source-map-loader": "^5.0.0",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "5.2.2",
    "xhr-mock": "^2.5.1"
  },
  "prettier": {
    "plugins": [
      "office-addin-prettier-config"
    ],
    "tabWidth": 2,
    "printWidth": 200
  },
  "browserslist": [
    "ie 11"
  ]
}
